function gui:setColor(a,b,c)
	if type(a)=="string" then
		self.Color=Color[a]
	elseif type(a)=="number" then
		self.Color=Color.new(a,b,c)
	end
end
function gui:setTextColor(a,b,c)
	if type(a)=="string" then
		self.TextColor=Color[a]
	elseif type(a)=="number" then
		self.TextColor=Color.new(a,b,c)
	end
end